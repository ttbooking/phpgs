{
  "name": "ttbooking/phpgs",
  "type": "library",
  "license": "MIT",
  "description": "PHP Wrapper for Ghost Script",
  "keywords": [
    "ghostscript",
    "ghost script",
    "gs",
    "pdf merge",
    "pdf split"
  ],
  "require": {
    "php": "^8.0",
    "symfony/process": "^6.0 || ^7.0"
  },
  "require-dev": {
    "ext-imagick": "*",
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Webit\\PHPgs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Webit\\PHPgs\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
